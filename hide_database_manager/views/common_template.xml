<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="inherited_login_layout" inherit_id="web.login_layout" name="My Theme Login"
                  priority="3">
            <xpath expr="//t[@t-call='web.frontend_layout']/t[last()]" position="after">
                 <t t-set="disable_database_manager" t-value="1"/>
                 <t t-set="disable_footer" t-value="1"/>
            </xpath>
        </template>
    </data>
</odoo>
